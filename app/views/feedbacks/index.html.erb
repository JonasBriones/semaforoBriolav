<h1>Feedback</h1>
<table id="feedback_table" class="display responsive no-wrap" >
  <thead>
    <tr>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Estado</th>
      <th>Respuestas</th>
      <th>Acci√≥n</th>
    </tr>
  </thead>
  <tbody>
    <% @polls.each do |p| %>
      <tr>
        <td><%= p.rut %></td>
        <td><%= p.nombre %></td>
        <td><%= p.state.nombre %></td>
        <td><%= p.feedbacks.count %></td>
        <td>
          <%= link_to image_tag('respuesta.png'), new_feedback_path( :id => p.id ), :class => 'btn btn-mini' %>
      </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Ir a graficos', polls_path, remote: true %>
